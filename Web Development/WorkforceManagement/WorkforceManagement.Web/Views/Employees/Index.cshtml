@model List<Employee>

<table class="table">
    <tr>
        <th>Name</th>
        <th>Gender</th>
        <th>Address</th>
        <th>Date of Birth</th>
        <th>Contact</th>
        <th></th>
    </tr>
    @foreach (Employee emp in Model)
    {
        <tr>
            <td>@emp.Name</td>
            <td>@emp.Gender</td>
            <td>@emp.Address</td>
            <td>@emp.Dob</td>
            <td>@emp.Contact</td>
            <td>
                <a href="/employees/details/@emp.Id"><i class="bi bi-eye"></i></a>
                <a href="/employees/edit/@emp.Id"><i class="bi bi-pen text-warning"></i></a>
                <a href="/employees/delete/@emp.Id"><i class="bi bi-trash text-danger"></i></a>
            </td>
        </tr>
    }
</table>
