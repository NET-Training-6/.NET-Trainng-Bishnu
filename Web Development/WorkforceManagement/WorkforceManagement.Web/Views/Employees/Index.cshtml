@model List<Employee>

<table class="table">
    <tr>
        <th>Name</th>
        <th>Gender</th>
        <th>Address</th>
        <th>Date of Birth</th>
        <th>Contact</th>
        <th>Join Date</th>
        <th>Department</th>
        <th>Designation</th>
    </tr>
    @foreach(Employee emp in Model)
    {
        <tr>
            <td>@emp.Name</td>
            <td>@emp.Gender</td>
            <td>@emp.Address</td>
            <td>@emp.Dob</td>
            <td>@emp.Contact</td>
            <td>@emp.JoinDate</td>
            <td>@emp.Department</td>
            <td>@emp.Designation</td>
        </tr>
    }    
</table>